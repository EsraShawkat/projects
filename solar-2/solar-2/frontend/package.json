{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npx http-server -p 8081 ./dist",
    "tests:unit": "vue-cli-service test::unit",
    "test:cleaner": "node_modules/.bin/jest --clearCache"
  },
  "dependencies": {
    "autoprefixer": "^10",
    "core-js": "^3.8.3",
    "http-server": "^14.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "postcss": "^8",
    "tailwindcss": "^3",
    "vue": "^3.2.13",
    "vue-router": "^4.2.4",
    "vue-sweetalert2": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.1.3",
    "tailwindcss": "^3.3.3"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "testMatch": [
      "**/test/**.spec.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "/images/(.*)$:": "<rootDir>/public/images/$1"
    },
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      "^.+\\.js$": "babel-jest",
      "\\.(png|jpg|jpeg|gif|svg)$": "<rootDir>/jest-media-transformer.js"
    },
    "verbose": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "vue/setup-compiler-macros": true,
      "jest": true
    },
    "overrides": [
      {
        "files": "**/src/davidTest.spec.js"
      }
    ],
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
